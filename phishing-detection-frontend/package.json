{
  "name": "phishing-detection-frontend",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production",
    "build:dev": "ng build --configuration development",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:ci": "ng test --watch=false --browsers=ChromeHeadless",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "analyze": "ng build --configuration production --stats-json && npx webpack-bundle-analyzer dist/phishing-detection-frontend/browser/stats.json",
    "serve:ssr": "node dist/phishing-detection-frontend/server/server.mjs",
    "prerender": "ng build && ng run phishing-detection-frontend:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "19.1.8",
    "@angular/cdk": "19.1.0",
    "@angular/common": "19.1.8",
    "@angular/compiler": "19.1.8",
    "@angular/core": "19.1.8",
    "@angular/forms": "19.1.8",
    "@angular/material": "19.1.0",
    "@angular/platform-browser": "19.1.8",
    "@angular/platform-browser-dynamic": "19.1.8",
    "@angular/platform-server": "19.1.8",
    "@angular/router": "19.1.8",
    "@angular/ssr": "19.1.8",
    "@swimlane/ngx-charts": "^23.0.1",
    "chart.js": "^4.4.6",
    "d3": "^7.9.0",
    "express": "^5.0.1",
    "gapi-script": "^1.2.0",
    "rxjs": "~7.8.1",
    "tslib": "^2.8.1",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "19.1.8",
    "@angular/cli": "19.1.8",
    "@angular/compiler-cli": "19.1.8",
    "@angular-eslint/builder": "19.1.0",
    "@angular-eslint/eslint-plugin": "19.1.0",
    "@angular-eslint/eslint-plugin-template": "19.1.0",
    "@angular-eslint/template-parser": "19.1.0",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "@types/express": "^5.0.0",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^22.10.0",
    "eslint": "^9.18.0",
    "eslint-plugin-import": "^2.31.0",
    "jasmine-core": "~5.4.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.7.0",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=11.0.0"
  }
}
